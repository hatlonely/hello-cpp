    "C_Cpp.default.includePath": [
        "${workspaceFolder}/build/vcpkg_installed/x64-linux/include",
        "${workspaceFolder}/build/vcpkg_installed/arm64-linux/include",
        "${workspaceFolder}/build/gcm.cache/usr/include/c++/11"
    ],
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.formatting": "clangFormat",
    "editor.formatOnSave": true,
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "testMate.cpp.test.advancedExecutables": [
        {
            "pattern": "build/**.test",
            "cwd": "${absDirpath}",
            "env": {
                "AWS_ACCESS_KEY_ID": "${env:AWS_ACCESS_KEY_ID}",
                "AWS_SECRET_ACCESS_KEY": "${env:AWS_SECRET_ACCESS_KEY}"
            }
        }
    ],
    "testMate.cpp.discovery.loadOnStartup": true,
    "C_Cpp.clang_format_sortIncludes": true,
    "C_Cpp.vcFormat.newLine.beforeCatch": false,
    "C_Cpp.vcFormat.newLine.beforeElse": false,
    "markdown.extension.print.theme": "dark",
    "markdown.extension.syntax.plainTheme": true,
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true
    },
    "files.autoSave": "onFocusChange",
    "terminal.integrated.scrollback": 10000,
    "files.insertFinalNewline": true,
    "notebook.insertFinalNewline": true,
    "C_Cpp.doxygen.generatedStyle": "//!"
